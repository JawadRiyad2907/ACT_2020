    <label class="control-label col-md-1" for="SearchFirstName">@Html.DisplayNameFor(x => x.FirstName)</label>
    <div class="col-md-3">
        <input class="form-control input-sm text-box single-line" id="SearchFirstName" type="text" value="">
    </div>
</div>
<div class="form-group row">
    <label class="control-label col-md-1" for="SearchLastName">@Html.DisplayNameFor(x => x.LastName)</label>
    <div class="col-md-3">
        <input class="form-control input-sm text-box single-line" id="SearchLastName" type="text" value="">
    </div>
</div>
<div class="form-group row">
    <label class="control-label col-md-1" for="SearchEmail">@Html.DisplayNameFor(x => x.Email)</label>
    <div class="col-md-3">
        <input class="form-control input-sm text-box single-line" id="SearchEmail" type="text" value="">
    </div>
</div>
<div class="row">
